<div class="d-flex flex-column align-items-center buttons-container-initial">
 <% @businesses.distinct.pluck(:category).each do |category| %>
      <%= button_to category.capitalize.gsub('_', ' ') + "s", meet_businesses_path(params[:meet_id]), method: :get, params: { query: category }, class:"btn btn-lg btn-dark rounded" %>
  <% end %>
</div>
